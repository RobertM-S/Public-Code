    "import numpy as np\n",
    "def E_len(x):\n",
    "    dotProduct = 0\n",
    "    for i in range(len(x)):\n",
    "        dotProduct = dotProduct + x[i]**2\n",
    "    return dotProduct **0.5\n",
    "print(E_len(np.array([3,4])))"
   ]
